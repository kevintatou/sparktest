{
  "name": "@sparktest/core",
  "version": "0.1.0",
  "description": "Shared TypeScript types and utilities for SparkTest",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    },
    "./samples": {
      "types": "./dist/samples.d.ts",
      "default": "./dist/samples.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "default": "./dist/constants/index.js"
    },
    "./constants/navigation": {
      "types": "./dist/constants/navigation.d.ts",
      "default": "./dist/constants/navigation.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./types/navigation": {
      "types": "./dist/types/navigation.d.ts",
      "default": "./dist/types/navigation.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "lucide-react": "^0.454.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5",
    "@types/node": "^22",
    "@types/react": "^18"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  }
}