{
  "name": "@sparktest/storage-service",
  "version": "0.1.0",
  "description": "Storage service implementations for SparkTest",
  "main": "index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./index.ts"
    },
    "./api-storage": {
      "types": "./api-storage.ts",
      "default": "./api-storage.ts"
    },
    "./local-storage": {
      "types": "./local-storage.ts",
      "default": "./local-storage.ts"
    },
    "./hybrid-storage": {
      "types": "./hybrid-storage.ts",
      "default": "./hybrid-storage.ts"
    },
    "./storage": {
      "types": "./storage.ts",
      "default": "./storage.ts"
    },
    "./dummy-definitions": {
      "types": "./dummy-definitions.ts",
      "default": "./dummy-definitions.ts"
    },
    "./generic": {
      "types": "./generic/index.ts",
      "default": "./generic/index.ts"
    },
    "./sparktest-storage": {
      "types": "./sparktest-storage.ts",
      "default": "./sparktest-storage.ts"
    }
  },
  "files": [
    "*.ts",
    "generic/"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@sparktest/core": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^22"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}