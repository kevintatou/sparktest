{
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": {
      "main": false
    }
  },
  "github": {
    "enabled": false
  },
  "functions": {
    "apps/oss/app/**": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
