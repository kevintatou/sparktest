{
  "version": 2,
  "buildCommand": "cd apps/oss && npm run build",
  "outputDirectory": "apps/oss/.next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rootDirectory": "apps/oss",
  "git": {
    "deploymentEnabled": {
      "main": false,
      "copilot/fix-142": false
    }
  },
  "github": {
    "enabled": false,
    "autoAlias": false,
    "autoJobCancelation": false
  },
  "builds": [
    {
      "src": "apps/oss/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "functions": {
    "apps/oss/app/**": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
